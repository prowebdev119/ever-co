{
	"expo": {
		"name": "ever-demand-shop-mobile-expo",
		"slug": "ever-demand-shop-mobile-expo",
		"owner": "everco",
		"description": "Ever Shop Mobile App build with expo",
		"version": "0.4.3",
		"orientation": "portrait",
		"primaryColor": "#2A2C39",
