			"barStyle": "light-content"
		},
		"android": {
			"package": "com.everDemand.shopMobileExpo",
			"versionCode": 1,
			"adaptiveIcon": {
				"foregroundImage": "./src/assets/img/appIcons/adaptive-icon.png",
				"backgroundColor": "#2A2C39"
			}
		},
		"web": {
			"favicon": "./src/assets/img/appIcons/favicon.png"
		}
